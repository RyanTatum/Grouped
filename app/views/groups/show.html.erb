<% # if @group_record["group_icon"]
   #@profile_picture = @group_record["group_icon"]["url"]
  #else
    @profile_picture = "default_group_icon.png"
  #end
%>

<body>
  <div class="container" id="view">
    <div class="row">
      <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad" >
        <div class="panel panel-info">
          <div class="panel-heading">
            <h3 class="panel-title">Group</h3>
          </div>
          <div class="panel-body">
            <div class="row">
              <div id= "group_pic" align="center"> 
                <%=image_tag @profile_picture, class:"img-circle img-responsive" %>
              </div>
              <div id= "group_info"> 
                <table class="table table-user-information">
                  <tbody>
                    <tr>
                      <td>Name</td>
                      <td><%= @group_record["name"] %></td>
                    </tr>
                    <tr>
                      <td>Created on</td>
                      <% dt = DateTime.new(@group_record["createdAt"][0..3].to_i, @group_record["createdAt"][5..6].to_i, @group_record["createdAt"][8..9].to_i) %>
                      <td><%= dt.strftime("%B") %>  <%= @group_record["createdAt"][5..6].to_i.ordinalize %>, <%=dt.strftime("%Y") %></td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
          <div id= "custom_footer" class="panel-footer">
            <%if @permissions_hash[session[:current_user]["objectId"]] == "group_admin"%>
              <span class="pull-right">
                <button id="edit" class="btn btn-sm btn-warning"><i class="glyphicon glyphicon-edit"></i></button>
              </span>
            <%end%>
          </div>
        </div>
      </div>
    </div>
  </div>


  <div class="container" id="editprofile" style="display: none">
    <div class="row">
      <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad" >
        <div class="panel panel-info">
          <div class="panel-heading">
            <h3 class="panel-title">Your Profile</h3>
          </div>
          <%= form_tag(group_path(@group_id), multipart: true, method: 'put') do %>
          <div class="panel-body">
            <div class="row">
              <div class="col-md-3 col-lg-3 " align="center"> 
                <%= image_tag @profile_picture, class:"img-circle img-responsive" %>
              </div>
              <div class=" col-md-9 col-lg-9 "> 
                <table class="table table-user-information">
                  <tbody>
                    
                    <tr>
                      <td>Name</td>
                      <td><%= text_field_tag(:new_name, @group_record["name"], :required => true, class: "form-control") %></td>
                    </tr>
                    <tr>
                      <td>Created on</td>
                      <td><%= dt.strftime("%B") %>  <%= @group_record["createdAt"][5..6].to_i.ordinalize %>, <%=dt.strftime("%Y") %></td>
                    </tr>
                    <tr>
                      <td>Group Icon</td>
                      <div class="form-group">
                        <td id= "createFile">
                          <span id= "form_cancel" class="btn btn-large btn-default btn-file">
                            Choose File
                            <%= file_field_tag :file %>
                          </span>
                        </td>
                      </div>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
          <div id= "custom_footer" class="panel-footer">
            <!--<span class="pull-right">-->
              <button type="button" id="cancel" class="btn btn-sm btn-danger button-right">
                <i class="glyphicon glyphicon-remove"></i>
              </button>
              <%= submit_tag("Save", class: 'btn btn-sm btn-success button-right', 'data-disable-with' => "Saving") %>
            <!--</span>-->
          </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>




<div class= "center_wrapper">
  <%if @permissions_hash[session[:current_user]["objectId"]] == "group_admin"%>
    <div class="project">
      <a href="https://icon.uiowa.edu"><%= image_tag "add.png", height: '100', width: '100' %></a>
      <a href="https://icon.uiowa.edu" style="text-decoration: none"><h2>Add Member</h2></a>
    </div>
  <%end%>  
  <% @users.each do |user| %>
  <div id = <%=@objectId_hash[user["user_objectId"]]%> class="project">
    <a href=<%= user_path(user["user_objectId"])%>><%= image_tag "project.png", height: '100', width: '100' %></a>
    <a href=<%= user_path(user["user_objectId"])%> style="text-decoration: none"><h2><%=user["first_name"]%> <%=user["last_name"]%></h2></a>
    <% d = DateTime.new(user["createdAt"][0..3].to_i, user["createdAt"][5..6].to_i, user["createdAt"][8..9].to_i) %>
    <%if @status_hash[user["user_objectId"]] == "pending"%>
      <div class= "center_wrapper">
        <p>Status: Pending</p>
      </div>
    <%else%>
      <p>Joined on <%= d.strftime("%B") %>  <%= user["createdAt"][5..6].to_i.ordinalize %>, <%=d.strftime("%Y") %> </p>
    <%end%>
  </div>
  <% end %>
</div>
</body>


